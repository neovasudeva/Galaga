module game_controller ( input         Clk,            
                                       Reset,          
                                       frame_clk,      
                         input [9:0]   DrawX, DrawY,   
                         input [7:0]	keycode,		
								 output  		data
);
	 logic is_user_ship;
	 logic [23:0] user_ship_data;
	 logic [9:0] x_pos, y_pos;
	 
	 logic is_enemy_ship;
	 logic [23:0] enemy_ship_data;
	 
	 logic is_laser;
	 logic [23:0] laser_data;
	 
	 assign data = 1'b1;
endmodule 